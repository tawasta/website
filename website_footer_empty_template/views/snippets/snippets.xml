<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="snippet_options_footer_empty" inherit_id="website.snippet_options">
        <xpath
            expr="//we-button[@data-customize-website-views='website.template_footer_headline']"
            position="after"
        >
            <we-button
                title="Empty"
                data-customize-website-views="website_footer_empty_template.template_footer_empty"
                data-customize-website-variable="'empty'"
                data-img="/website/static/src/img/snippets_options/footer_template_headline.svg"
            />
        </xpath>
    </template>
</odoo>
