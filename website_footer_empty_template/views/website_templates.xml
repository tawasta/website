<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="template_footer_empty"
        inherit_id="website.layout"
        name="Empty"
        active="False">
        <xpath expr="//div[@id='footer']" position="replace">
            <div
                id="footer"
                class="oe_structure oe_structure_solo"
                t-ignore="true"
                t-if="not no_footer">
                <div class="oe_structure oe_empty" />
            </div>
        </xpath>
    </template>
</odoo>
