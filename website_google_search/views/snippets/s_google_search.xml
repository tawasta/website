<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template id="s_google_search" name="Google Search">
        <section class="s_google_search_box mt32 mb32">
                <div class="row">
                    <div class="col-lg-12">
                        <t
                        t-if="request.env['website'].get_current_website().has_google_search">
                            <div
                            class="gcse-searchbox-only"
                            data-resultsUrl="/search"
                        />
                        </t>
                        <t t-else="">
                            <div
                            class="alert alert-warning alert-dismissible text-center">Google Search is not enabled on this website. </div>
                        </t>
                    </div>
                </div>
        </section>
    </template>
</odoo>
