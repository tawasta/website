<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="placeholder_header_google_search"
        name="Placeholder Header Language Selector"
    />
    <template
        id="header_google_search"
        inherit_id="website_google_search.placeholder_header_google_search"
        name="Header Google Search"
        active="False">
        <xpath expr="." position="inside">
            <div id="header_google_search_box" class="js_google_search">
                <div class="gcse-searchbox-only" data-resultsUrl="/search" />
            </div>
        </xpath>
    </template>
    <template id="google_search_results" name="Google Search Results">
        <t t-call="website.layout">
            <div class="gcse-searchresults-only" />
        </t>
    </template>
    <template id="template_header_default" inherit_id="website.template_header_default">
        <xpath
            expr="//t[@t-call='website.placeholder_header_language_selector']"
            position="after">
            <t t-call="website_google_search.placeholder_header_google_search" />
        </xpath>
    </template>
