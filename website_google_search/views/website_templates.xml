<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="placeholder_header_google_search"
        name="Placeholder Header Google Search"
    />
    <template
        id="placeholder_header_google_search_list_item"
        name="Placeholder Header Google Search List Item"
    />
    <template
        id="header_google_search"
        inherit_id="website_google_search.placeholder_header_google_search"
        name="Header Google Search"
        active="False"
    >
        <xpath expr="." position="inside">
            <div
                id="header_google_search_box"
                t-attf-class="js_google_search o_no_autohide_item #{_div_classes}"
            >
                <div class="gcse-searchbox-only" data-resultsUrl="/search" />
            </div>
        </xpath>
    </template>
    <template
        id="header_google_search_list_item"
        inherit_id="website_google_search.placeholder_header_google_search_list_item"
        name="Header Google Search List Item"
        active="False"
    >
        <xpath expr="." position="inside">
            <li
                id="header_google_search_box"
                t-attf-class="js_google_search o_no_autohide_item #{_div_classes}"
                style="min-width: 250px;"
            >
                <div class="gcse-searchbox-only" data-resultsUrl="/search" />
            </li>
        </xpath>
    </template>
    <template id="google_search_results" name="Google Search Results">
        <t t-call="website.layout">
            <div id="wrap" class="oe_structure oe_empty">
                <section class="s_google_search pt32 pb32">
                    <div class="container">
                        <div class="gcse-searchresults-only" />
                    </div>
                </section>
            </div>
        </t>
    </template>
    <template id="template_header_default" inherit_id="website.template_header_default">
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item mx-lg-3'" />
            </t>
        </xpath>
    </template>
    <template
        id="template_header_hamburger"
        inherit_id="website.template_header_hamburger"
    >
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item'" />
            </t>
        </xpath>
    </template>
    <template
        id="template_header_vertical"
        inherit_id="website.template_header_vertical"
    >
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item ml-lg-3'" />
            </t>
        </xpath>
    </template>
    <template id="template_header_sidebar" inherit_id="website.template_header_sidebar">
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item mr-auto'" />
            </t>
        </xpath>
    </template>
    <template id="template_header_slogan" inherit_id="website.template_header_slogan">
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item ml-3'" />
            </t>
        </xpath>
    </template>
    <template
        id="template_header_contact"
        inherit_id="website.template_header_contact_oe_structure_header_contact_1"
    >
        <xpath
            expr="//div[@id='oe_structure_header_contact_1']//section//div[hasclass('container')]//div[hasclass('row')]//div[hasclass('col-lg-8')]"
            position="attributes"
        >
            <attribute name="class">col-lg-6</attribute>
        </xpath>
        <xpath
            expr="//div[@id='oe_structure_header_contact_1']//section//div[hasclass('container')]//div[hasclass('row')]//div[hasclass('col-lg-4')]"
            position="before"
        >
            <t t-call="website_google_search.placeholder_header_google_search">
                <t t-set="_div_classes" t-value="'col-auto flex-fill'" />
            </t>
        </xpath>
        <xpath
            expr="//div[@id='oe_structure_header_contact_1']//section//div[hasclass('container')]//div[hasclass('row')]//div[hasclass('col-lg-4')]"
            position="attributes"
        >
            <attribute name="class">col-auto text-lg-right</attribute>
        </xpath>
    </template>
    <template
        id="template_header_minimalist"
        inherit_id="website.template_header_minimalist"
    >
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item ml-auto'" />
            </t>
        </xpath>
    </template>
    <template id="template_header_boxed" inherit_id="website.template_header_boxed">
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item ml-lg-3'" />
            </t>
        </xpath>
    </template>
    <template
        id="template_header_centered_logo"
        inherit_id="website.template_header_centered_logo"
    >
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item ml-3'" />
            </t>
        </xpath>
    </template>
    <template id="template_header_image" inherit_id="website.template_header_image">
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item ml-lg-auto'" />
            </t>
        </xpath>
    </template>
    <template
        id="template_header_hamburger_full"
        inherit_id="website.template_header_hamburger_full"
    >
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item'" />
            </t>
        </xpath>
    </template>
    <template
        id="template_header_magazine"
        inherit_id="website.template_header_magazine"
    >
        <xpath expr="//t[@t-call='portal.user_dropdown']" position="after">
            <t
                t-call="website_google_search.placeholder_header_google_search_list_item"
            >
                <t t-set="_div_classes" t-value="'nav-item'" />
            </t>
        </xpath>
    </template>
</odoo>
