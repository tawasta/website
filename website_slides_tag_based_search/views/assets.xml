<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <template
        id="assets_frontend"
        inherit_id="website_slides.assets_frontend"
        name="Tag-based slide search assets"
    >
        <xpath expr="//script[last()]" position="after">
            <script
                type="text/javascript"
                src="/website_slides_tag_based_search/static/src/js/website_slides_tag_based_search.js"
            />
        </xpath>

        <xpath expr="link[last()]" position="after">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/website_slides_tag_based_search/static/src/scss/website_slides_tag_based_search.scss"
            />
        </xpath>

    </template>
</odoo>
