<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="assets_frontend" name="Website Utilities assets" inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <!-- Plugins from cdn -->
            <link rel="stylesheet" type="text/css" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css"/>
            <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>

            <!-- Plugin initializing -->
            <script type="text/javascript" src="/website_utilities/static/src/js/loader.js"></script>
            <script type="text/javascript" src="/website_utilities/static/src/js/notifications.js"></script>
        </xpath>
    </template>
</odoo>
